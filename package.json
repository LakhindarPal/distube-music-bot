{
  "name": "distube-music-bot",
  "version": "0.1.2",
  "description": "A music bot for discord built using distube and discord.js",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "doc": "node scripts/cmd-doc.js",
    "build": "podman build -t lakhindarpal/distube-music-bot:latest .",
    "run": "podman run --env-file .env lakhindarpal/distube-music-bot:latest",
    "register": "node scripts/register.js",
    "start": "node ."
  },
  "engines": {
    "node": "20.x"
  },
  "keywords": [
    "distube",
    "distubejs",
    "bot",
    "discord",
    "music",
    "discordjs",
    "discord.js",
    "music-bot"
  ],
  "author": "Lakhindar Pal",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LakhindarPal/distube-music-bot.git"
  },
  "bugs": {
    "url": "https://github.com/LakhindarPal/distube-music-bot/issues"
  },
  "homepage": "https://github.com/LakhindarPal/distube-music-bot#readme",
  "dependencies": {
    "@discordjs/voice": "^0.17.0",
    "@distube/deezer": "^2.0.1",
    "@distube/direct-link": "^1.0.1",
    "@distube/file": "^1.0.1",
    "@distube/soundcloud": "^2.0.3",
    "@distube/spotify": "^2.0.2",
    "@distube/youtube": "^1.0.4",
    "discord.js": "^14.15.3",
    "distube": "^5.0.2",
    "distube-tidal": "^0.1.0",
    "dotenv": "^16.4.5",
    "genius-lyrics": "^4.4.7",
    "glob": "^10.4.5",
    "opusscript": "^0.1.1",
    "sodium-native": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "prettier": "3.3.2"
  },
  "directories": {
    "doc": "docs"
  }
}
